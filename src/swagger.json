        "summary": "Fetch list of teams",
        "tags": ["Teams"],
        "requestBody":{
          "content":{
            "application/json":{
              "schema":{
                "$ref":"#/components/schemas/GetTeamRequestModel"
              },
              "example":{
                "project_id":"12345",
                "key":"role",
                "value":"developer"
              }
