    "/teams":{
      "post":{
        "summary": "Create team",
        "tags": ["Teams"],
        "parameters": [
          {
            "name": "project_id",
            "in": "query",
            "description": "Project ID",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],

        "requestBody":  {
          "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/TeamRequestModel"
            },
            "example": {
              "team_members": [{
                  "username":"user1",
                  "role":"backend",
                  "key_status": true,
                  "start_date": "07/04/2023",
                  "end_date": "07/05/2023",
                  "cost":5000
              }
          ]}
          }
        }
      },
        "responses":{
          "201":{
            "description": "Team creation successful",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TeamModel"
                },
                "example": [
                  {
                      "id": 18,
                      "username": "user1",
                      "role": "backend",
                      "key_status": true,
                      "start_date": "2023-07-03T18:30:00.000Z",
                      "end_date": "2023-07-04T18:30:00.000Z",
                      "cost": 5000,
                      "project_id": "1bc70683-7528-4dc2-bb6a-ca95e65fb08b",
