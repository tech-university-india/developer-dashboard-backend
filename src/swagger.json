        "summary": "Create team",
        "tags": ["Teams"],
        "requestBody":  {
          "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/TeamRequestModel"
            },
            "example": {
              "project_id":"12345",
              "username":"ashutosh_senapati",
              "adder_role":"manager",
              "emp_role":"developer"
            }
          }
        }
      },
        "responses":{
          "201":{
            "description": "Team creation successful",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TeamModel"
                },
                "example": {
                  "id": 4,
                  "project_id": "12345",
                  "username": "ashutosh_senapati",
                  "role": "developer",
                  "emp_status": "active",
                  "updatedAt": "2023-02-18T17:09:47.067Z",
                  "createdAt": "2023-02-18T17:09:47.067Z"
                }
              }
            }
          }
        }
      },
